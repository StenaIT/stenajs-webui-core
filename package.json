{
  "name": "storybook-ts",
  "version": "1.0.0",
  "description": "testing storybook",
  "main": "index.js",
  "scripts": {
    "clean": "npm run clean-build && npm run clean-storybook",
    "clean-build": "rimraf build",
    "clean-storybook": "rimraf storybok-static",
    "build": "npm run lint && npm run clean && npm run build-lib && build-storybook",
    "build-lib-watch": "tsc -w",
    "build-lib": "npm run clean-build && tsc",
    "test": "react-scripts-ts test --env=jsdom",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts-ts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "npm run clean-storybook && build-storybook",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@storybook/react": "^3.4.2",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/react": "^16.3.11",
    "awesome-typescript-loader": "^4.0.0",
    "babel-core": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "prettier": "^1.12.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "@storybook/addon-info": "^3.4.2",
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "react": "^16.3.2",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.3.2",
    "react-scripts-ts": "^2.15.1",
    "ts-jest": "^22.4.4"
  }
}
