{
  "name": "stenajs-webui-core",
  "version": "0.9.0-alpha-15",
  "description": "A library of React components made by Stena IT.",
  "sideEffects": false,
  "repository": "https://github.com/StenaIT/stenajs-webui-common",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "start-storybook -p 6006",
    "start-from-scratch": "rm -rf node_modules && yarn && yarn build-index && yarn build-lib && yarn start",
    "prepublishOnly": "yarn && yarn build-index && yarn lint && CI=true yarn test && yarn build-lib",
    "deploy-storybook": "storybook-to-ghpages",
    "clean": "rimraf lib && rimraf storybok-static",
    "build-index": "cti -e @types,typings,__test__,__tests__ -i setupTests.ts,react-app-env.d.ts ./src",
    "build": "rimraf lib && yarn tsc --build tsconfig.prod.json && build-storybook",
    "build-lib-watch": "yarn tsc -w --build tsconfig.prod.json",
    "build-lib": "rimraf lib && yarn tsc --build tsconfig.prod.json",
    "test": "react-scripts test",
    "test:coverage": "yarn test -- --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "rimraf storybook-static && build-storybook",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prettier": "yarn prettier:src",
    "prettier:src": "prettier --parser typescript --trailing-comma all --single-quote --write 'src/**/*.ts' 'src/**/*.tsx'"
  },
  "author": "Stena IT <mattias.andersson@stenaline.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@dump247/storybook-state": "^1.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@storybook/addon-actions": "5.0.0",
    "@storybook/addon-info": "^5.0.0",
    "@storybook/react": "5.0.0",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^24.0.9",
    "@types/lodash": "^4.14.108",
    "@types/node": "^11.10.4",
    "@types/rc-slider": "^8.6.3",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/recompose": "^0.26.1",
    "@types/storybook__addon-info": "^4.1.0",
    "@types/storybook__react": "^4.0.0",
    "babel-loader": "^8.0.4",
    "create-ts-index": "^1.0.14",
    "emotion": "10.0.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "^24.1.0",
    "jest-emotion": "^10.0.7",
    "prettier": "1.16.4",
    "rc-slider": "^8.6.1",
    "react": "16.8.4",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "16.8.4",
    "react-emotion": "10.0.0",
    "react-scripts": "^2.1.2",
    "react-select": "2.4.1",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "3.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.2.0-6",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "@types/react-click-outside": "^3.0.2",
    "@types/react-select": "2.0.13",
    "@types/styled-system": "^3.2.2",
    "create-emotion": "^10.0.7",
    "date-fns": "2.0.0-alpha.26",
    "lodash": "^4.17.11",
    "react-click-outside": "^3.0.1",
    "react-popper": "^1.0.2",
    "react-spinkit": "^3.0.0",
    "recompose": "^0.27.0",
    "styled-system": "^4.0.1"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-svg-core": "*",
    "emotion": "*",
    "react": "*",
    "react-dom": "*",
    "react-emotion": "*"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
