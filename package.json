{
  "name": "stenajs-webui-core",
  "version": "0.9.0",
  "description": "A library of React components made by Stena IT.",
  "sideEffects": false,
  "repository": "https://github.com/StenaIT/stenajs-webui-common",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "start-storybook -p 6006",
    "start-from-scratch": "rm -rf node_modules && yarn && yarn build-index && yarn build-lib && yarn start",
    "prepublishOnly": "yarn && yarn build-index && yarn lint && CI=true yarn test && yarn build-lib",
    "deploy-storybook": "storybook-to-ghpages",
    "clean": "rimraf lib && rimraf storybok-static",
    "check-circular-deps": "yarn build-lib && madge --circular --extensions js ./lib",
    "build-index": "cti -e @types,typings,__test__,__tests__ -i setupTests.ts,react-app-env.d.ts ./src",
    "build": "yarn build-lib && yarn build-storybook",
    "build-lib-watch": "yarn tsc -w --build tsconfig.prod.json",
    "build-lib": "rimraf lib && NODE_OPTIONS=\"--max-old-space-size=4096\" yarn tsc --build tsconfig.prod.json",
    "test": "react-scripts test",
    "test:coverage": "yarn test -- --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "rimraf storybook-static && build-storybook",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' && eslint '{src,stories}/**/*.{ts,tsx}'",
    "print-node-version": "node --version",
    "prettier": "prettier --write 'src/**/*.ts?(x)' --ignore-path .gitignore"
  },
  "author": "Stena IT <mattias.andersson@stenaline.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@dump247/storybook-state": "^1.5.0",
    "@emotion/core": "10.0.7",
    "@emotion/styled": "10.0.7",
    "@fortawesome/free-brands-svg-icons": "5.7.2",
    "@storybook/addon-actions": "4.1.4",
    "@storybook/addon-info": "^4.1.4",
    "@storybook/react": "4.1.4",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.3.11",
    "@types/node": "^10.12.18",
    "@types/rc-slider": "^8.2.3",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__react": "^4.0.0",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-loader": "^8.0.4",
    "create-ts-index": "^1.0.14",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-emotion": "^9.2.7",
    "lint-staged": "^8.1.5",
    "lodash": "4.17.10",
    "madge": "^3.4.4",
    "prettier": "1.14.2",
    "rc-slider": "^8.6.1",
    "react": "16.8.3",
    "react-docgen-typescript-loader": "^2.2.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "16.8.3",
    "react-scripts": "^2.1.2",
    "react-select": "2.1.1",
    "recompose": "0.30.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.5.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@emotion/core": "10.0.7",
    "@emotion/styled": "10.0.7",
    "@fortawesome/fontawesome": "1.2.0-6",
    "@fortawesome/fontawesome-svg-core": "1.2.15",
    "@fortawesome/free-regular-svg-icons": "5.3.1",
    "@fortawesome/free-solid-svg-icons": "5.3.1",
    "@fortawesome/react-fontawesome": "0.1.2",
    "@types/lodash": "^4.14.108",
    "@types/react": "16.8.5",
    "@types/react-click-outside": "^3.0.2",
    "@types/react-dom": "16.8.2",
    "@types/react-select": "2.0.6",
    "@types/recompose": "^0.26.1",
    "@types/styled-system": "^3.2.2",
    "date-fns": "2.0.0-alpha.26",
    "lodash": "4.17.10",
    "react": "16.8.3",
    "react-click-outside": "^3.0.1",
    "react-dom": "16.8.3",
    "react-popper": "^1.0.2",
    "react-spinkit": "^3.0.0",
    "recompose": "0.30.0",
    "styled-system": "^4.0.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
